PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

SELECT *
{
    ?country rdf:type <http://dbpedia.org/ontology/Country> .
    ?country rdfs:label ?label .
    ?country <http://dbpedia.org/property/gdpNominal> ?gdp .
    ?country <http://dbpedia.org/property/populationEstimate> ?population .
    # FILTER (xsd:integer(?gdp) > 0)
    FILTER (LANG(?label) = 'en')
    FILTER (DATATYPE(?population) = xsd:integer)
}
ORDER BY DESC(xsd:integer(?gdp))
OFFSET 0
LIMIT 50