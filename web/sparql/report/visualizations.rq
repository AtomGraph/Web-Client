PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX vis: <http://code.google.com/apis/visualization/>
PREFIX semrep: <http://www.semantic-web.dk/ontologies/semantic-reports/>

SELECT *
{
	?report		rdf:type		semrep:Report .
	?report		semrep:visualizedBy	?visualization .
	?visualization	rdf:type		?type
	FILTER (?report = <%s>)
}