PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX vis: <http://code.google.com/apis/visualization/>
PREFIX rep: <http://www.semantic-web.dk/ontologies/semantic-reports/>

SELECT DISTINCT ?bindingType ?type
{
	?super          rdfs:subClassOf vis:Binding .
	?bindingType    rdfs:subClassOf ?super .
        ?super          rdfs:subClassOf ?rest .
        ?rest           owl:onProperty  vis:dataType .
        ?rest           owl:onClass     ?type
}