<!DOCTYPE rdf:RDF [
    <!ENTITY gc     "http://graphity.org/gc#">
    <!ENTITY gp     "http://graphity.org/gp#">
    <!ENTITY rdf    "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <!ENTITY rdfs   "http://www.w3.org/2000/01/rdf-schema#">
    <!ENTITY owl    "http://www.w3.org/2002/07/owl#">
    <!ENTITY xsd    "http://www.w3.org/2001/XMLSchema#">
    <!ENTITY foaf   "http://xmlns.com/foaf/0.1/">
    <!ENTITY sp     "http://spinrdf.org/sp#">
]>
<rdf:RDF xmlns:gp="&gp;" xmlns:rdf="&rdf;" xmlns:rdfs="&rdfs;" xmlns:owl="&owl;" xmlns:foaf="&foaf;" xmlns:sp="&sp;">
    <rdf:Description rdf:nodeID="this">
        <rdf:type rdf:resource="&foaf;Document"/>
        <rdf:type rdf:resource="&gp;Item"/>
        <foaf:primaryTopic rdf:nodeID="thing"/>
        <gp:slug rdf:datatype="&xsd;string"> </gp:slug>
    </rdf:Description>
    <rdf:Description rdf:nodeID="thing">
        <rdf:type rdf:resource="&sp;Query"/>
        <foaf:isPrimaryTopicOf rdf:nodeID="this"/>
        <rdfs:label> </rdfs:label>
        <sp:text rdf:datatype="&xsd;string">DESCRIBE ?this</sp:text>
    </rdf:Description>
</rdf:RDF>