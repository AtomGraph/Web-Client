#@base <http://linkeddata.dk/> . # needs to be explicit when deploying, because of missing :8080 port number

@prefix : <ontology/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix g: <http://graphity.org/ontology/> .
@prefix sd: <http://www.w3.org/ns/sparql-service-description#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix sp: <http://spinrdf.org/sp#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix sioc: <http://rdfs.org/sioc/ns#> .
@prefix lda: <http://purl.org/linked-data/api/vocab#>.
@prefix gfb-app: <http://graph.facebook.com/schema/application#> .

: a owl:Ontology ;
    dc:title "Graphity Browser ontology" ;
    # owl:imports <http://graphity.org/ontology/> ;
    rdfs:seeAlso <http://graphity.org> .

_:dataset a void:Dataset;
    foaf:homepage <> ;
    rdfs:isDefinedBy : ;
    void:sparqlEndpoint <http://de.dydra.com/heltnormalt/testing/sparql> .
_:endpoint a sd:Service ;
    rdfs:isDefinedBy : ;
    sd:endpoint <http://de.dydra.com/heltnormalt/testing/sparql> ;
    sd:supportedLanguage sd:SPARQL11Query .
<https://graph.facebook.com/121081534640971#> gfb-app:id "121081534640971"^^xsd:string ;
    gfb-app:name "Graphity Facebook app" ;
    rdfs:label "Graphity Facebook app" .

<> a sioc:Site, sioc:Forum, lda:API, lda:ListEndpoint ;
    dc:title "Graphity Browser" ;
    g:query g:ConstructLatest ;
    rdfs:isDefinedBy : ;
    void:inDataset _:dataset ;
    lda:sparqlEndpoint <http://de.dydra.com/heltnormalt/testing/sparql> ;
    g:facebookApp <https://graph.facebook.com/121081534640971#> ;
    g:defaultQuery g:Describe .

<classes> a sioc:Forum, lda:ListEndpoint ;
    sioc:has_host <> ;
    dc:title "Classes"@en ;
    g:query g:ConstructLatest ;
    rdfs:isDefinedBy : ;
    void:inDataset _:dataset .
[] a lda:ItemEndpoint ;
  lda:uriTemplate "classes/{name}" .
