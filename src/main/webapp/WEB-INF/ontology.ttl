<queries/denmark/schools/vanl%C3%B8se>    a       sp:Select, sp:Query, sioc:Post ;
    g:service _:SchoolsInDenmark ;
    dct:title "Skoler i Vanl첩se"@da ;
    dct:title "Schools in Vanl첩se"@en ;
    dct:created "2012-06-11"^^xsd:date ;
    dct:description "Danske skoler med postnummer 2720"@da ;
    dct:description "Danish schools with postal code 2720"@en ;
    dct:subject <http://dbpedia.org/resource/Denmark> ,
	<http://dbpedia.org/resource/School> ,
	<http://dbpedia.org/resource/Copenhagen> ,
	<http://dbpedia.org/resource/Vanl%C3%B8se> ;

    sp:text """PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX dbpedia-owl: <http://dbpedia.org/ontology/>
PREFIX dc: <http://purl.org/dc/elements/1.1/>

SELECT DISTINCT ?school ?title ?address
WHERE
{
  GRAPH ?g
  {
    ?school dbpedia-owl:postalCode "2720" ;
      dc:title ?title ;
      dbpedia-owl:address ?address
  }
}
ORDER BY ?title
LIMIT 50"""^^xsd:string .

