@prefix : <ontology/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix g: <http://graphity.org/ontology/> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix sp: <http://spinrdf.org/sp#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix sioc: <http://rdfs.org/sioc/ns#> .
@prefix dbpedia: <http://dbpedia.org/resource/> .
@prefix lda: <http://purl.org/linked-data/api/vocab#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .

: a owl:Ontology ;
    owl:imports <http://graphity.org/ontology/> ;
    dct:title "Graphity Browser ontology" ;
    rdfs:label "Graphity Browser ontology" ;
    dct:modified "2012-02-11T03:17:00+01:00"^^xsd:dateTime ;
    dct:license <http://opendatacommons.org/licenses/odbl/1.0/> ;
    dct:creator <http://semantic-web.dk#martynas> ;
    rdfs:seeAlso <sparql> .

_:dataset a void:Dataset;
    foaf:homepage <> ;
    rdfs:isDefinedBy : ;
    void:sparqlEndpoint <sparql> .

<http://graphity.org#browser> a doap:Project ;
    dct:title "Graphity Browser" ;
    doap:repository <https://github.com/Graphity/graphity-browser.git> ;
    doap:browse <https://github.com/Graphity/graphity-browser> ;
    doap:bug-database <https://github.com/Graphity/graphity-browser/issues> ;
    doap:maintainer <http://semantic-web.dk#martynas> ;
    doap:license <http://usefulinc.com/doap/licenses/gpl> ;
    doap:programming-language dbpedia:XSLT,
	<http://dbpedia.org/resource/Java_%28programming_language%29> .

# RESOURCES

<> a sioc:Site, sioc:Forum, lda:API, lda:ListEndpoint ;
    dct:title "Graphity Browser" ;
    dct:description "Generic Linked Data and SPARQL endpoint browser"@en ;
    dct:subject dbpedia:Linked_Data,
	dbpedia:Resource_Description_Framework ,
	dbpedia:SPARQL ,
	dbpedia:Semantic_web ,
	dbpedia:Web_browser ,
	dbpedia:Open_source ;
    dct:creator <http://semantic-web.dk#martynas> ;
    # g:query g:SelectServices ;
    rdfs:isDefinedBy : ;
    void:inDataset _:dataset .

<sparql> a rdf:Resource ;
    sioc:has_host <> ;
    dct:title "SPARQL endpoint"@en ;
    rdfs:isDefinedBy : .

<search> a lda:ListEndpoint ;
    sioc:has_host <> ;
    dct:title "Search"@en ;
    g:query g:ConstructRegex ;
    rdfs:isDefinedBy : ;
    void:inDataset _:dataset ;
    g:service _:service .

<ontologies> a sioc:Forum, lda:ListEndpoint ;
    sioc:has_host <> ;
    dct:title "Ontologies"@en ;
    g:query g:SelectOntologies ;
    rdfs:isDefinedBy : ;
    void:inDataset _:dataset ;
    g:service _:service .

<classes> a sioc:Forum, lda:ListEndpoint ;
    sioc:has_host <> ;
    dct:title "Classes"@en ;
    g:query g:SelectClasses ;
    rdfs:isDefinedBy : ;
    void:inDataset _:dataset .

<queries> a sioc:Forum, lda:ListEndpoint ;
    sioc:has_host <> ;
    dct:title "Queries"@en ;
    g:query g:SelectQueries ;
    rdfs:isDefinedBy : ;
    void:inDataset _:dataset .