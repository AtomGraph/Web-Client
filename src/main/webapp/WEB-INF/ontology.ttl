#@base <http://linkeddata.dk/> . # needs to be explicit when deploying, because of missing :8080 port number

@prefix : <ontology/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix g: <http://graphity.org/ontology/> .
@prefix sd: <http://www.w3.org/ns/sparql-service-description#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix sp: <http://spinrdf.org/sp#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix sioc: <http://rdfs.org/sioc/ns#> .
@prefix lda: <http://purl.org/linked-data/api/vocab#> .
@prefix gfb-app: <http://graph.facebook.com/schema/application#> .

: a owl:Ontology ;
    owl:imports <http://graphity.org/ontology/> ;
    dc:title "Graphity Browser ontology" ;
    rdfs:label "Graphity Browser ontology" ;
    dct:modified "2012-02-11T03:17:00+01:00"^^xsd:dateTime ;
    dct:license <http://opendatacommons.org/licenses/odbl/1.0/> ;
    dc:creator <http://semantic-web.dk#martynas> .

_:dataset a void:Dataset;
    foaf:homepage <> ;
    rdfs:isDefinedBy : ;
    void:sparqlEndpoint <http://de.dydra.com/graphity/browser/sparql> .
_:service a sd:Service ;
    rdfs:isDefinedBy : ;
    sd:endpoint <http://de.dydra.com/graphity/browser/sparql> ;
    dc:title "Default Graphity SPARQL endpoint";
    g:apiKey "2hwmvJSgCSV3fa3xGIb9"^^xsd:string ;
    sd:supportedLanguage sd:SPARQL11Query .
<https://graph.facebook.com/121081534640971#> gfb-app:id "121081534640971"^^xsd:string ;
    gfb-app:name "Graphity Facebook app" ;
    rdfs:label "Graphity Facebook app" .

<http://data.kasabi.com/dataset/airports> a void:Dataset ;
    dc:title "Airports" ;
    void:sparqlEndpoint <http://api.kasabi.com/dataset/airports/apis/sparql> .
_:airports-service a sd:Service ;
    sd:endpoint <http://api.kasabi.com/dataset/airports/apis/sparql> ;
    g:apiKey "ad4afcb0f52e872b3923b5d254bb07844f224cc5"^^xsd:string .

<> a sioc:Site, sioc:Forum, lda:API, lda:ListEndpoint ;
    dc:title "Graphity Browser" ;
    g:query g:ConstructLatest ;
    rdfs:isDefinedBy : ;
    void:inDataset _:dataset ;
    lda:sparqlEndpoint <http://de.dydra.com/graphity/browser/sparql> ;
    g:facebookApp <https://graph.facebook.com/121081534640971#> ;
    g:defaultQuery g:Describe .

<classes> a sioc:Forum, lda:ListEndpoint ;
    sioc:has_host <> ;
    dc:title "Classes"@en ;
    g:query g:ConstructLatestClasses ;
    rdfs:isDefinedBy : ;
    void:inDataset _:dataset .
#[] a lda:ItemEndpoint ;
#  lda:uriTemplate "classes/{name}" .

:ClassItem rdfs:subClassOf
[
	a owl:Restriction;
	owl:onProperty sioc:has_container;
	owl:hasValue <classes>
] ,
[
	a owl:Restriction;
	owl:onProperty g:query;
	owl:hasValue :ConstructItem
] .