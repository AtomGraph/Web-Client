# QUERIES

_:b1  sp:varName "obs"^^xsd:string .
_:b2  sp:varName "date"^^xsd:string .

<queries/politiken/polls>    a       sp:Select, sp:Query, sioc:Post ;
    dct:title "Meningsm책linger folketingsvalg"@da ;
    dct:title "Danish parliament polls"@en ;
    dct:description "Udvikling af antallet af partiers mandater over tid"@da ;
    dct:description "Development of parliamentary mandates by parties over time"@en ;
    dct:created "2012-05-11"^^xsd:date ;
    dct:subject <http://dbpedia.org/resource/Denmark> ,
	<http://dbpedia.org/resource/Opinion_poll> ,
	<http://dbpedia.org/resource/Folketing> ,
	<http://dbpedia.org/resource/Election> ;

      #g:service _:Politiken ;
      vis:visualizedBy [ a vis-google:LineChart ;
                        vis:binding [ a vis-google:LineChartLabelBinding ;
			                vis:variable _:b2 # date on X
                                    ] ,
				    [ a vis-google:LineChartValueBinding ;
			                vis:variable _:b4 # seats on Y
                                    ]
			] ;
      sp:text """PREFIX mm: <http://politiken.dk/politik/meningsmaaleren/>
PREFIX party: <http://politiken.dk/politik/parti/>
PREFIX qb: <http://purl.org/linked-data/cube#>
PREFIX sdmx-dimension: <http://purl.org/linked-data/sdmx/2009/dimension#>
SELECT ?date ((AVG(?A)) AS ?A) ((AVG(?B)) AS ?B) ((AVG(?C)) AS ?C) ((AVG(?F)) AS ?F) ((AVG(?K)) AS ?K) ((AVG(?O)) AS ?O) ((AVG(?OE)) AS ?OE) ((AVG(?V)) AS ?V) ((AVG(?Y)) AS ?Y)
WHERE {
    GRAPH ?g {
        ?obs a qb:Observation .
        ?obs sdmx-dimension:refPeriod ?date .
        OPTIONAL {
            ?obs mm:party party:A .
            ?obs mm:seatCount ?A .
        } .
        OPTIONAL {
            ?obs mm:party party:B .
            ?obs mm:seatCount ?B .
        } .
        OPTIONAL {
            ?obs mm:party party:C .
            ?obs mm:seatCount ?C .
        } .
        OPTIONAL {
            ?obs mm:party party:F .
            ?obs mm:seatCount ?F .
        } .
        OPTIONAL {
            ?obs mm:party party:K .
            ?obs mm:seatCount ?K .
        } .
        OPTIONAL {
            ?obs mm:party party:O .
            ?obs mm:seatCount ?O .
        } .
        OPTIONAL {
            ?obs mm:party party:OE .
            ?obs mm:seatCount ?OE .
        } .
        OPTIONAL {
            ?obs mm:party party:V .
            ?obs mm:seatCount ?V .
        } .
        OPTIONAL {
            ?obs mm:party party:Y .
            ?obs mm:seatCount ?Y .
        } .
    } .
}
GROUP BY ?date
ORDER BY DESC(?date)"""^^xsd:string .

