: a owl:Ontology ;
    owl:imports <http://graphity.org/ontology/> ;
    dct:title "Linked Data Browser ontology" ;
    rdfs:label "Linked Data Browser ontology" ;
    dct:created "2012-02-11T03:17:00+01:00"^^xsd:dateTime ;
    dct:modified "2012-07-17T10:11:00+01:00"^^xsd:dateTime ;
    dct:license <http://opendatacommons.org/licenses/odbl/1.0/> ;
    dct:creator <http://semantic-web.dk#martynas> ;
    rdfs:seeAlso <sparql> .

_:dataset a void:Dataset ;
    dct:title "The dataset of this application" ;
    foaf:homepage <> ;
    rdfs:isDefinedBy : ;
    void:sparqlEndpoint <sparql> .

# RESOURCES

<> a sioc:Site, sioc:Forum, lda:API, lda:ListEndpoint ;
    dct:title "Linked Data Browser" ;
    dct:description "This application is a basic example of how a generic Linked Data and SPARQL endpoint browser could look like. You're welcome to use it as it is, or as a starting point for your own Linked Data web application. If you come up with something reusable, don't forget to contribute back!" ;
    dct:subject dbpedia:Linked_Data,
	dbpedia:Resource_Description_Framework ,
	dbpedia:SPARQL ,
	dbpedia:Web_browser ;
    dct:creator <http://semantic-web.dk#martynas> ;
    g:query g:SelectServices ;
    doap:implements [ a doap:Project ;
	dct:title "Graphity Browser" ;
	doap:name "Graphity Browser" ;
	dct:created "2012-01-05"^^xsd:date ;
	doap:homepage <http://graphity.org> ;
	dct:subject dbpedia:Linked_Data ,
	    dbpedia:Resource_Description_Framework ,
	    dbpedia:SPARQL ,
	    dbpedia:Semantic_web ,
	    <http://dbpedia.org/resource/Category:Web_application_frameworks> ,
	    dbpedia:Open_source ;
	dct:hasPart <http://dbpedia.org/resource/Jena_%28framework%29> ,
	    dbpedia:Saxon_XSLT ;
	doap:release [ a doap:Version ;
	    dct:title "Graphity Browser 1.0" ;
	    doap:revision "1.0.0" ;
	    doap:file-release <https://github.com/Graphity/graphity-browser/zipball/v1.0.0> ,
		<https://github.com/Graphity/graphity-core/graphity-browser/v1.0.0> ] ;
	doap:file-release <https://github.com/Graphity/graphity-browser/zipball/master> ,
	    <https://github.com/Graphity/graphity-browser/tarball/master> ;
	doap:repository [ a doap:GitRepository ;
	    dct:title "Graphity Browser source code repository" ;
	    doap:location <https://github.com/Graphity/graphity-browser.git> ;
	    doap:browse <https://github.com/Graphity/graphity-browser> ] ;
	doap:bug-database <https://github.com/Graphity/graphity-browser/issues> ;
	doap:maintainer <http://semantic-web.dk#martynas> ;
	doap:license dbpedia:GNU_General_Public_License ;
	dbpedia-owl:programmingLanguage dbpedia:XSLT,
	    <http://dbpedia.org/resource/Java_%28programming_language%29> ] ;
    rdfs:isDefinedBy : ;
    void:inDataset _:dataset .

<sparql> a rdf:Resource ;
    sioc:has_host <> ;
    dct:title "SPARQL endpoint" ;
    rdfs:isDefinedBy : .

<post> a rdf:Resource ;
    sioc:has_host <> ;
    dct:title "New post" ;
    dct:description "An example of a form with RDF/POST encoding, which can be used for a complete read/write RDF roundtrip" ;
    rdfs:isDefinedBy : ;
    void:inDataset _:dataset .

<ontologies> a sioc:Forum, lda:ListEndpoint ;
    sioc:has_host <> ;
    dct:title "Ontologies" ;
    dct:description "An example of a resource selecting ontologies using SPARQL query generated from SPIN RDF descriptions" ;
    g:query g:SelectOntologies ;
    rdfs:isDefinedBy : ;
    void:inDataset _:dataset .

<classes> a sioc:Forum, lda:ListEndpoint ;
    sioc:has_host <> ;
    dct:title "Classes" ;
    dct:description "An example of a resource selecting classes using SPARQL query generated from SPIN RDF descriptions" ;
    g:query g:SelectClasses ;
    rdfs:isDefinedBy : ;
    void:inDataset _:dataset .

# SERVICES / DATASETS

_:DBPedia a void:Dataset, sd:Service ;
    dct:title "DBPedia" ;
    dct:description "RDF data extracted from Wikipedia" ;
    foaf:homepage <http://dbpedia.org/> ;
    foaf:logo <http://wiki.dbpedia.org/images/dbpedia_logo.png> ;
    void:sparqlEndpoint <http://dbpedia.org/sparql> ;
    sd:endpoint <http://dbpedia.org/sparql> ;
    void:exampleResource <http://dbpedia.org/resource/Copenhagen> ,
	<http://dbpedia.org/resource/Physics> ,
	<http://dbpedia.org/resource/Ludwig_van_Beethoven> ;
    sd:supportedLanguage sd:SPARQL11Query ;
    void:triples "550000000"^^xsd:integer .

_:Reegle a void:Dataset, sd:Service ;
