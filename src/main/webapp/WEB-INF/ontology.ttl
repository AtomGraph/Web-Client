<queries/politiken/polls/annual>    a       sp:Select, sp:Query, sioc:Post ;
    dct:title "Meningsm책linger folketingsvalg (책rligt)"@da ;
    dct:title "Danish parliament polls (annual)"@en ;
    dct:description "Udvikling af antallet af hovedpartiers mandater over de sidste 책r"@da ;
    dct:description "Development of parliamentary mandates of main parties over the last years"@en ;
    dct:created "2012-06-14"^^xsd:date ;
    dct:subject <http://dbpedia.org/resource/Denmark> ,
	<http://dbpedia.org/resource/Opinion_poll> ,
	<http://dbpedia.org/resource/Folketing> ,
	<http://dbpedia.org/resource/Election> ;

      #g:service _:Politiken ;
      vis:visualizedBy [ a vis-google:ColumnChart ;
                        vis:binding [ a vis-google:ColumnChartLabelBinding ;
			                vis:variable _:b2 # date on X
                                    ] ,
				    [ a vis-google:ColumnChartValueBinding ;
			                vis:variable _:b4 # seats on Y
                                    ]
			] ;
      sp:text """PREFIX mm: <http://politiken.dk/politik/meningsmaaleren/>
PREFIX party: <http://politiken.dk/politik/parti/>
PREFIX qb: <http://purl.org/linked-data/cube#>
PREFIX sdmx-dimension: <http://purl.org/linked-data/sdmx/2009/dimension#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
SELECT ?year ((AVG(?Ac)) AS ?A) ((AVG(?Bc)) AS ?B) ((AVG(?Oc)) AS ?O) ((AVG(?Vc)) AS ?V)
WHERE {
    GRAPH ?g {
        ?obs a qb:Observation .
        ?obs sdmx-dimension:refPeriod ?date .
        BIND(year(?date) AS ?year)
        OPTIONAL {
            ?obs mm:party party:A .
            ?obs mm:seatCount ?Ac .
        } .
        OPTIONAL {
            ?obs mm:party party:B .
            ?obs mm:seatCount ?Bc .
        } .
        OPTIONAL {
            ?obs mm:party party:O .
            ?obs mm:seatCount ?Oc .
        } .
        OPTIONAL {
            ?obs mm:party party:V .
            ?obs mm:seatCount ?Vc .
        } .
        OPTIONAL {
            ?obs mm:party party:Y .
            ?obs mm:seatCount ?Yc .
        } .
    } .
}
GROUP BY ?year
ORDER BY ?year"""^^xsd:string .

