        gp:uriTemplate "/properties/labelled" ;
        spin:query [ a gp:DescribeLabelPropertiesTemplate ] ;
        gp:limit 20 ;
        gp:orderBy "label"^^xsd:string ;
        rdfs:label "Labelled properties container" ;
        rdfs:isDefinedBy <#> .

}