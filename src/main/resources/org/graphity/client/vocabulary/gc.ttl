: a owl:Ontology ;
    owl:imports gp: ;
    owl:versionInfo "1.0.7" ;
    dct:title "Graphity Client ontology" ;
    rdfs:label "Graphity Client ontology" ;
    dct:description "Graphity Linked Data client ontology. Should be imported and reused by all extending applications" ;
    dct:created "2012-10-26T01:04:00+03:00"^^xsd:dateTime ;
    dct:modified "2012-12-19T02:29:00+03:00"^^xsd:dateTime, "2013-02-22T12:10:00+03:00"^^xsd:dateTime ;
    vann:preferredNamespaceUri "http://client.graphity.org/ontology#" ;
    vann:preferredNamespacePrefix "gc";
    foaf:homepage <http://client.graphity.org/ontology> ;
    foaf:isPrimaryTopicOf <ontology> ;
    dct:license <http://www.opendatacommons.org/licenses/pddl/1.0/> ;
    dct:creator <http://semantic-web.dk/#martynas> ;
    rdfs:seeAlso :Project . # <sparql> .

:dataset a void:Dataset ;
    dct:title "Graphity Client dataset" ;
    # foaf:homepage <dataset> ;
    rdfs:isDefinedBy : .
    # void:sparqlEndpoint <sparql> .

# RESOURCES

<..> a sioc:Site, ldp:Container, foaf:Document ;
    dct:title "Graphity Client" ;
    rdfs:isDefinedBy : ;
    void:inDataset :dataset ;
    rdfs:seeAlso : .

<ontology> a foaf:Document ;
    foaf:primaryTopic : ;
    rdfs:isDefinedBy : ;
    void:inDataset :dataset .

<dataset> a foaf:Document ;
    foaf:primaryTopic :dataset ;
    rdfs:isDefinedBy : ;
    void:inDataset :dataset .

<resources> a ldp:Container, sioc:Container ;
    sioc:has_host <..> ;
    dct:title "Resources" ;
    rdfs:isDefinedBy : ;
    void:inDataset :dataset .

<instances> a ldp:Container, sioc:Container ;
    sioc:has_host <..> ;
    dct:title "Instances" ;
    rdfs:isDefinedBy : ;
    void:inDataset :dataset .

<ontologies> a ldp:Container, sioc:Container ;
    sioc:has_host <..> ;
