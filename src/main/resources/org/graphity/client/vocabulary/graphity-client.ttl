    doap:programming-language dbpedia:XSLT,
	<http://dbpedia.org/resource/Java_%28programming_language%29> .

# PROPERTIES

ldp:pageOf a owl:ObjectProperty ;
    rdfs:domain ldp:Page ;
    rdfs:range ldp:Container ;
    rdfs:isDefinedBy ldp: .

# RESOURCES

<..> a sioc:Site, foaf:Document ;
    dct:title "Graphity Linked Data Platform" ;
    rdfs:isDefinedBy : ;
    void:inDataset :dataset ;
    rdfs:seeAlso : .

<../resources> a ldp:Container, sioc:Container ;
    sioc:has_host <..> ;
    dct:title "Resources" ;
    foaf:primaryTopic : ;
    rdfs:isDefinedBy : ;
    void:inDataset :dataset .

<../instances> a ldp:Container, sioc:Container ;
    sioc:has_host <..> ;
    dct:title "Instances" ;
    rdfs:isDefinedBy : ;
    void:inDataset :dataset .

<../ontologies> a ldp:Container, sioc:Container ;
    sioc:has_host <..> ;
    dct:title "Ontologies" ;
    rdfs:isDefinedBy : ;
    void:inDataset :dataset .

<../classes> a ldp:Container, sioc:Container ;
    sioc:has_host <..> ;
    dct:title "Classes" ;
    rdfs:isDefinedBy : ;
    void:inDataset :dataset .

<../properties> a ldp:Container, sioc:Container ;
    sioc:has_host <..> ;
    dct:title "Properties" ;
    rdfs:isDefinedBy : ;
    void:inDataset :dataset .

<../datasets> a ldp:Container, sioc:Container ;
    sioc:has_host <..> ;
    dct:title "Datasets" ;
    rdfs:isDefinedBy : ;
    void:inDataset :dataset .

<../services> a ldp:Container, sioc:Container ;
    sioc:has_host <..> ;
    dct:title "Services" ;
    rdfs:isDefinedBy : ;
    void:inDataset :dataset .

<../queries> a ldp:Container, sioc:Container ;
    sioc:has_host <..> ;
    dct:title "Queries" ;
    rdfs:isDefinedBy : ;
    void:inDataset :dataset .

<../sparql> a foaf:Document ;
    dct:title "SPARQL endpoint" ;
    rdfs:isDefinedBy : ;
    void:inDataset :dataset .

# RESOURCE CLASSES

:Resource a owl:Class ;
    rdfs:subClassOf foaf:Document, sioc:Item, lda:ItemEndpoint ,
	[ a owl:Restriction ;
		owl:onProperty sioc:has_host ;
		owl:hasValue <..>
	] ,
	[ a owl:Restriction ;
		owl:onProperty void:inDataset ;
		owl:hasValue :dataset
	] ,
	[ a owl:Restriction ;
		owl:onProperty lda:uriTemplate ;
		owl:hasValue "/{path: .*}"
	] ;
    spin:constraint [ a :DescribeTemplate ] ;
    rdfs:label "Resource" ;
    rdfs:isDefinedBy : .

