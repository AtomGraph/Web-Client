@base <http://graphity.org/ontologies/visualizations/google> .

@prefix : <#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix vis: <http://graphity.org/ontologies/visualizations#> .

<> a owl:Ontology ;
    rdfs:label "Google Visualization API Ontology"@en ;
    owl:imports <http://graphity.org/ontologies/visualizations> .

# OBJECT PROPERTIES

:option a owl:ObjectProperty ;
    rdfs:domain vis:Visualization ;
    rdfs:range :Option ;
    rdfs:isDefinedBy : .

:package a owl:ObjectProperty ;
    rdfs:domain vis:Visualization ;
    rdfs:range :Package ;
    rdfs:isDefinedBy : .

# DATATYPE PROPERTIES

:name a owl:DatatypeProperty ;
    rdfs:domain :Option ;
    rdfs:range xsd:string ;
    rdfs:isDefinedBy : .

:order a owl:DatatypeProperty ;
    rdfs:label "Binding order"@en ;
    rdfs:domain :Binding ;
    rdfs:range xsd:integer ;
    rdfs:isDefinedBy : .

# CLASSES

:Option a owl:Class ;
    rdfs:label "Customization option"@en ;
    rdfs:isDefinedBy : .

:Package a owl:Class ;
    rdfs:label "Visualization package"@en ;
    rdfs:isDefinedBy : .

# Visualizations

:AreaChart a owl:Class ;
    rdfs:label "Area chart"@en ;
    rdfs:subClassOf vis:AreaChart ,
    [ a owl:Restriction ;
	owl:onProperty :binding ;
	owl:onClass :AreaChartLabelBinding ;
	owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
    ] ,
    [ a owl:Restriction ;
	owl:onProperty :binding ;
	owl:someValuesFrom :AreaChartValueBinding
    ] ,
    [ a owl:Restriction ;
	owl:onProperty :package ;
	owl:hasValue :CoreChartPackage
    ] ;
    rdfs:isDefinedBy : .

:BarChart a owl:Class ;
    rdfs:label "Bar chart"@en ;
    rdfs:subClassOf vis:BarChart ,
    [ a owl:Restriction ;
	owl:onProperty :binding ;
	owl:someValuesFrom :BarChartValueBinding
    ] ,
    [ a owl:Restriction ;
	owl:onProperty :binding ;
	owl:onClass :BarChartLabelBinding ;
	owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
    ] ,
    [ a owl:Restriction ;
	owl:onProperty :package ;
	owl:hasValue :CoreChartPackage
    ] ;
    rdfs:isDefinedBy : .

:ColumnChart a owl:Class ;
    rdfs:label "Column chart"@en ;
    rdfs:subClassOf vis:ColumnChart ,
    [ a owl:Restriction ;
	owl:onProperty :binding ;
	owl:someValuesFrom :ColumnChartValueBinding
    ] ,
    [ a owl:Restriction ;
	owl:onProperty :binding ;
	owl:onClass :ColumnChartLabelBinding ;
	owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
    ] ,
    [ a owl:Restriction ;
	owl:onProperty :package ;
	owl:hasValue :CoreChartPackage
    ] ;
    rdfs:isDefinedBy : .

:LineChart a owl:Class ;
    rdfs:label "Line chart"@en ;
    rdfs:subClassOf vis:LineChart ,
    [ a owl:Restriction ;
	owl:onProperty :binding ;
	owl:someValuesFrom :LineChartValueBinding
    ] ,
    [ a owl:Restriction ;
	owl:onProperty :binding ;
	owl:onClass :LineChartLabelBinding ;
	owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
    ] ,
    [ a owl:Restriction ;
	owl:onProperty :package ;
	owl:hasValue :CoreChartPackage
    ] ;
    rdfs:isDefinedBy : .

:Map a owl:Class ;
    rdfs:label "Map"@en ;
    rdfs:subClassOf vis:Map ,
    [ a owl:Restriction ;
	owl:onProperty :binding ;
	owl:onClass :MapLngBinding ;
	owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
    ] ,
    [ a owl:Restriction ;
	owl:onProperty :binding ;
	owl:onClass :MapLabelBinding ;
	owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger
    ] ,
    [ a owl:Restriction ;
	owl:onProperty :binding ;
	owl:onClass :MapLatBinding ;
	owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
    ] ,
    [ a owl:Restriction ;
	owl:onProperty :package ;
	owl:hasValue :MapPackage
    ] ;
    rdfs:isDefinedBy : .

:PieChart a owl:Class ;
    rdfs:label "Pie chart"@en ;
    rdfs:subClassOf vis:PieChart ,
    [ a owl:Restriction ;
	owl:onProperty :binding ;
	owl:onClass :PieChartValueBinding ;
	owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
    ] ,
    [ a owl:Restriction ;
	owl:onProperty :binding ;
	owl:onClass :PieChartLabelBinding ;
	owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
    ] ,
    [ a owl:Restriction ;
	owl:onProperty :package ;
	owl:hasValue :CoreChartPackage
    ] ;
    rdfs:isDefinedBy : .

:ScatterChart a owl:Class ;
    rdfs:label "Scatter chart"@en ;
    rdfs:subClassOf vis:ScatterChart ,
    [ a owl:Restriction ;
	owl:onProperty :binding ;
	owl:someValuesFrom :ScatterChartYBinding
    ] ,
    [ a owl:Restriction ;
	owl:onProperty :binding ;
	owl:onClass :ScatterChartXBinding ;
	owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
    ] ,
    [ a owl:Restriction ;
	owl:onProperty :package ;
	owl:hasValue :CoreChartPackage
    ] ;
    rdfs:isDefinedBy : .

:Table a owl:Class ;
    rdfs:subClassOf vis:Table ,
    [ a owl:Restriction ;
	owl:onProperty :package ;
	owl:hasValue :TablePackage
    ] ;
    rdfs:isDefinedBy : .

# Options

:ChartTitle a owl:Class ;
    rdfs:label "Title of the chart"@en ;
    rdfs:subClassOf :Option ,
    [ a owl:Restriction ;
	owl:onProperty :name ;
	owl:hasValue "title"
    ] ,
    [ a owl:Restriction ;
	owl:onProperty :value ;
	owl:allValuesFrom xsd:string
    ] ;
    rdfs:isDefinedBy : .

:HAxisTitle a owl:Class ;
    rdfs:label "Title of the horizotal axis"@en ;
    rdfs:subClassOf :Option ,
    [ a owl:Restriction ;
	owl:onProperty :value ;
	owl:allValuesFrom xsd:string
    ] ,
    [ a owl:Restriction ;
	owl:onProperty :name ;
	owl:hasValue "hAxis.title"^^xsd:string
    ] ;
    rdfs:isDefinedBy : .

:VAxisTitle a owl:Class ;
    rdfs:label "Title of the vertical axis"@en ;
    rdfs:subClassOf :Option ,
    [ a owl:Restriction ;
	owl:onProperty :value ;
	owl:allValuesFrom xsd:string
    ] ,
    [ a owl:Restriction ;
	owl:onProperty :name ;
	owl:hasValue "vAxis.title"^^xsd:string
    ] ;
    rdfs:isDefinedBy : .

# Bindings

:AreaChartLabelBinding a owl:Class ;
    rdfs:label "Label binding"@en ;
    rdfs:subClassOf :StringBinding ,
    [ a owl:Restriction ;
	owl:onProperty :order ;
	owl:hasValue 0
    ] ;
    rdfs:isDefinedBy : .

:AreaChartValueBinding a owl:Class ;
    rdfs:label "Value binding"@en ;
    rdfs:subClassOf :NumericBinding ;
    rdfs:isDefinedBy : .

:BarChartLabelBinding a owl:Class ;
    rdfs:label "Label binding"@en ;
    rdfs:subClassOf :StringBinding ,
    [ a owl:Restriction ;
	owl:onProperty :order ;
	owl:hasValue 0
    ] ;
    rdfs:isDefinedBy : .

:BarChartValueBinding a owl:Class ;
    rdfs:label "Value binding"@en ;
    rdfs:subClassOf :NumericBinding ;
    rdfs:isDefinedBy : .

:ColumnChartLabelBinding a owl:Class ;
    rdfs:label "Label binding"@en ;
    rdfs:subClassOf :StringBinding ,
    [ a owl:Restriction ;
	owl:onProperty :order ;
	owl:hasValue 0
    ] ;
    rdfs:isDefinedBy : .

:ColumnChartValueBinding a owl:Class ;
    rdfs:label "Value binding"@en ;
    rdfs:subClassOf :NumericBinding ;
    rdfs:isDefinedBy : .

:LineChartLabelBinding a owl:Class ;
    rdfs:label "Label binding"@en ;
    rdfs:subClassOf :StringBinding ,
    [ a owl:Restriction ;
	owl:onProperty :order ;
	owl:hasValue 0
    ] ;
    rdfs:isDefinedBy : .

:LineChartValueBinding a owl:Class ;
    rdfs:label "Value binding"@en ;
    rdfs:subClassOf :NumericBinding ;
    rdfs:isDefinedBy : .

:MapAddressBinding a owl:Class ;
    rdfs:label "Address binding"@en ;
    rdfs:subClassOf :StringBinding ,
    [ a owl:Restriction ;
	owl:onProperty :order ;
	owl:hasValue 0
    ] ;
    rdfs:isDefinedBy : .

:MapLabelBinding a owl:Class ;
    rdfs:label "Label binding"@en ;
    rdfs:subClassOf :StringBinding ,
    [ a owl:Restriction ;
	owl:onProperty :order ;
	owl:hasValue 2
    ] ;
    rdfs:isDefinedBy : .

:MapLatBinding a owl:Class ;
    rdfs:label "Latitude binding"@en ;
    rdfs:subClassOf :NumericBinding ,
    [ a owl:Restriction ;
	owl:onProperty :order ;
	owl:hasValue 0
    ] ;
    rdfs:isDefinedBy : .

:MapLngBinding a owl:Class ;
    rdfs:label "Longitude binding"@en ;
    rdfs:subClassOf :NumericBinding ,
    [ a owl:Restriction ;
	owl:onProperty :order ;
	owl:hasValue 1
    ] ;
    rdfs:isDefinedBy : .

:PieChartLabelBinding a owl:Class ;
    rdfs:label "Label binding"@en ;
    rdfs:subClassOf :StringBinding ,
    [ a owl:Restriction ;
	owl:onProperty :order ;
	owl:hasValue 0
    ] ;
    rdfs:isDefinedBy : .

:PieChartValueBinding a owl:Class ;
    rdfs:label "Value binding"@en ;
    rdfs:subClassOf :NumericBinding ,
    [ a owl:Restriction ;
	owl:onProperty :order ;
	owl:hasValue 1
    ] ;
    rdfs:isDefinedBy : .

:ScatterChartXBinding a owl:Class ;
    rdfs:label "X binding"@en ;
    rdfs:subClassOf :NumericBinding ,
    [ a owl:Restriction ;
	owl:onProperty :order ;
	owl:hasValue 0
    ] ;
    rdfs:isDefinedBy : .

:ScatterChartYBinding a owl:Class ;
    rdfs:label "Y binding"@en ;
    rdfs:subClassOf :NumericBinding ;
    rdfs:isDefinedBy : .

# INDIVIDUALS

:CoreChartPackage a :Package .
:MapPackage a :Package .
:TablePackage a :Package .